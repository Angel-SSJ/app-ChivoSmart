{
  "expo": {
    "name": "Chivo Smart",
    "slug": "Chivo-Smart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-logo-testing.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon":"./assets/images/icon-logo-testing.png",
      "package": "com.fabricio23r.fintechapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1C1C1D"
        }
      ],
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
